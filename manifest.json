{
  "name": "Mobx Prettier",
  "manifest_version": 3,
  "version": "0.0.0",
  "description": "Prettify Mobx objects in Chrome console and debugger.",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_title": "Mobx Prettier",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_script.min.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "page_script.min.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
